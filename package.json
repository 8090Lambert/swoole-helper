{
	"name": "vscode-swoole-helper",
	"displayName": "VScode-swoole-helper",
	"description": "swoole helper with VScode",
	"author": "juzhensheng <juzs215@gmail.com>",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets",
		"Other"
	],
	"keywords": [
		"php",
		"swoole",
		"php-swoole",
		"swoole-helper"
	],
	"activationEvents": [
		"onLanguage:php",
		"workspaceContains:**/.php"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@commitlint/cli": "7.2.1",
		"@commitlint/config-conventional": "7.1.2",
		"@types/execa": "0.9.0",
		"@types/mocha": "^2.2.42",
		"@types/mz": "0.0.32",
		"@types/node": "^10.12.21",
		"@types/semver": "5.5.0",
		"eslint": "^5.13.0",
		"husky": "1.1.3",
		"prettier": "1.15.3",
		"semantic-release": "^17.2.3",
		"semantic-release-vsce": "2.1.2",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"execa": "1.0.0",
		"mz": "2.7.0",
		"semver": "5.6.0",
		"vscode-languageclient": "5.1.1"
	},
	"license": "MIT"
}
